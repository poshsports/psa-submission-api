{
  "version": 2,
  "routes": [
    { "src": "^/api/admin-login$",  "methods": ["POST"], "dest": "/api/proxy/admin-login.js" },
    { "src": "^/api/admin-logout$", "methods": ["POST"], "dest": "/api/proxy/admin-logout.js" },
    { "src": "^/api/submit$",       "methods": ["POST"], "dest": "/api/submit.js" },
    { "src": "^/admin/?$", "dest": "/admin/index.html" },
    { "src": "^/(?:index\\.html)?$", "dest": "/admin/index.html" }
  ]
}
